# Personal account - the default config
Host bitbucket.org
   HostName bitbucket.org
   User git
   IdentityFile ~/.ssh/id_rsa

# Nyk acocunt
Host bitbucket.org-nyk
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/nyk

# Personal account - the default config
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_rsa

