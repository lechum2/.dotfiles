# Personal account - the default config
Host bitbucket.org
   HostName bitbucket.org
   User git
   IdentityFile ~/.ssh/id_rsa

# Cloudity account
Host bitbucket.org-cloudity
   HostName bitbucket.org
   User git
   IdentityFile ~/.ssh/cloudity

# Personal account - the default config
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_rsa

# Cloudity account
Host github.com-cloudity
   HostName github.com
   User git
   IdentityFile ~/.ssh/cloudity